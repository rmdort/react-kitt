{
  "name": "react-kitt",
  "version": "1.1.1",
  "description": "Super lightweight React User Inteface components",
  "main": "lib/index.js",
  "scripts": {
    "docs": "styleguidist server",
    "docs:build": "styleguidist build",
    "standard": "standard --verbose --fix",
    "watch": "BABEL_ENV=production babel --watch src --out-dir lib",
    "prepublish": "NODE_ENV=production BABEL_ENV=production babel src --out-dir lib && npm run copy:scss",
    "postpublish": "rimraf lib",
    "copy:scss": "gulp --gulpfile scripts/gulpfile.js",
    "lint": "eslint src",
    "pages": "npm run docs:build && buildbranch gh-pages docs",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"src/**/!(__tests__)/*.js\"",
    "test": "jest",
    "test:watch": "yarn test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rmdort/react-kitt"
  },
  "bugs": {
    "url": "https://github.com/rmdort/react-kitt/issues"
  },
  "homepage": "https://github.com/rmdort/react-kitt",
  "pre-commit": [
    "prettier",
    "test"
  ],
  "prettier": {
    "semi": false
  },
  "keywords": [
    "react",
    "ui",
    "framework",
    "light",
    "simple",
    "small"
  ],
  "author": "Vinay M <rmdort@gmail.com>",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/test",
      "/lib",
      "/node_modules",
      "/example",
      "/docs"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|svg|css|scss)$": "<rootDir>/tests/__mocks__/fileMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tests/__mocks__/setup.js"
  },
  "peerDependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "flexboxgrid": "^6.3.1",
    "hint.css": "^2.5.0",
    "mitt": "^1.1.2",
    "normalize.css": "^7.0.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-line-progress": "^1.0.3",
    "react-modal": "^3.1.4",
    "react-onclickoutside": "^6.7.0",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "buildbranch": "^2.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.5.1",
    "gulp": "^3.9.1",
    "jest": "^21.2.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "pre-commit": "^1.2.2",
    "precss": "^2.0.0",
    "prettier": "1.8.2",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react-styleguidist": "^6.0.33",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "tailwindcss": "^0.2.2",
    "webpack": "^3.8.1"
  }
}
